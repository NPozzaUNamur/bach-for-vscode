{
  "name": "bach-for-vscode",
  "displayName": "Bach for VSCode",
  "description": "Add Bach language support in VSCode.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mbach",
      "aliases": ["Bach", "bach"],
      "extensions": [".mbach"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mbach",
      "scopeName": "source.mbach",
      "path": "./syntaxes/mbach.tmLanguage.json"
    }]
  }
}
